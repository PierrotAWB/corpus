{
  "name": "corpus",
  "version": "0.0.9000-pre",
  "description": "A note-management application for macOS",
  "main": "dist/main/index.js",
  "repository": "https://github.com/wincent/corpus.git",
  "author": "Greg Hurrell <greg@hurrell.net>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "electron": "5.0.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-react": "7.14.2",
    "prettier": "*",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "build": "tsc",
    "ci": "yarn format:check && yarn lint && yarn test",
    "clean": "rm -r dist/*",
    "format": "prettier --write -- '*.{js,json,md}' 'src/**/*.{html,ts,tsx}'",
    "format:check": "prettier --list-different -- '*.{js,json,md}' 'src/**/*.{html,ts,tsx}'",
    "lint": "eslint '.*.js' 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix '.*.js' 'src/**/*.{ts,tsx}'",
    "start": "electron .",
    "test": "true",
    "watch": "tsc --watch"
  }
}
