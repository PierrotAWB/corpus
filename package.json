{
  "name": "corpus",
  "version": "0.0.9000-pre",
  "description": "A note-management application for macOS",
  "main": "dist/main.js",
  "repository": "https://github.com/wincent/corpus.git",
  "author": "Greg Hurrell <greg@hurrell.net>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "electron": "5.0.0"
  },
  "devDependencies": {
    "prettier": "*",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -r dist/*",
    "format": "prettier --write -- '*.{json,md}' 'src/**/*.{html,ts}'",
    "format:check": "prettier --list-different -- '*.{json,md}' 'src/**/*.{html,ts}'",
    "start": "electron ."
  }
}
