{
  "name": "Corpus",
  "version": "0.0.9000-pre",
  "description": "A note-management application for macOS",
  "main": "dist/main/index.js",
  "repository": "https://github.com/wincent/corpus.git",
  "author": "Greg Hurrell <greg@hurrell.net>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@wincent/clamp": "*",
    "@wincent/frozen-set": "*",
    "@wincent/throttle": "*",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "electron": "^6.0.2",
    "electron-installer-dmg": "3.0.0",
    "electron-packager": "14.0.4",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-notice": "^0.8.8",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "jest": "^24.8.0",
    "prettier": "*",
    "rollup": "^1.19.4",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "ts-jest": "^24.0.2",
    "typescript": "3.7.1-rc",
    "yarn-deduplicate": "^1.1.1"
  },
  "scripts": {
    "build": "tsc",
    "ci": "yarn format:check && yarn lint && yarn typecheck && yarn test && yarn deduplicate:check",
    "clean": "rm -r dist/*",
    "deduplicate": "yarn-deduplicate yarn.lock",
    "deduplicate:check": "yarn-deduplicate yarn.lock -f",
    "fix": "yarn format && yarn lint:fix && yarn deduplicate",
    "format": "prettier --write -- '*.{js,json,md}' 'src/**/*.{html,ts,tsx}'",
    "format:check": "prettier --list-different -- '*.{js,json,md}' 'src/**/*.{html,ts,tsx}'",
    "lint": "eslint '*.js' 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix '*.js' 'src/**/*.{ts,tsx}'",
    "release": "yarn build && node package.js",
    "start": "electron .",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "watch": "tsc --watch"
  }
}
